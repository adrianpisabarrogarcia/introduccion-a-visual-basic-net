Public Class Alumno

    Property nombre As String
    Property primerApellido As String
    Property segundoApellido As String

    Sub New(nombre As String, primerApellido As String, segundoApellido As String)
        Me.nombre = nombre
        Me.primerApellido = primerApellido
        Me.segundoApellido = segundoApellido
    End Sub


End Class
